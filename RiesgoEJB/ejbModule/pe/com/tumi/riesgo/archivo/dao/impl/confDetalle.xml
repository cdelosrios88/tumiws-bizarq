<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="ConfDetalleDaoIbatis">

  <typeAlias type="pe.com.tumi.riesgo.archivo.domain.ConfDetalle" alias="ConfDetalle"/> 
	
  <resultMap id="resConfDetalle" class="ConfDetalle" >
     <result property="id.intPersEmpresaPk"      		column="pPers_empresa_n_pk" />
     <result property="id.intItemConfiguracion"    		column="pItem_archivoconf_n" />
     <result property="id.intItemConfiguracionDetalle"  column="pItem_archivoconfdet_n" />
     <result property="intParaTipoDatoCod"   			column="pPara_tipodato_n_cod" />
     <result property="intParaTipoValorCod"   			column="pPara_tipovalor_n_cod" />
     <result property="intParaTipoDatoVariableCod"      column="pPara_tipodatovariable_n_cod" />
     <result property="strDatoFijo"    					column="pDatofijo_v" />
     <result property="strValorCampo"      				column="pValorcampo_v" />
     <result property="intTamano"   					column="pTamano_n" />
     <result property="intParaTipoAlineacionCod"   		column="pPara_tipoalineacion_n_cod" />
     <result property="intParaTipoCompletaCod"   		column="pPara_tipocompleta_n_cod" />
     <result property="intParaEstadoCod"   				column="pPara_estado_n_cod" />
  </resultMap>
  
  <parameterMap id="parConfDetalle" class="ConfDetalle">
     <parameter property="id.intPersEmpresaPk"				jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
	 <parameter property="id.intItemConfiguracion"			jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>    
     <parameter property="id.intItemConfiguracionDetalle" 	jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>  
     <parameter property="intParaTipoDatoCod" 				jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="intParaTipoValorCod" 				jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="intParaTipoDatoVariableCod" 		jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="strDatoFijo" 						jdbcType="VARCHAR"  javaType="java.lang.String" 	mode="INOUT"/>
     <parameter property="strValorCampo" 					jdbcType="VARCHAR"  javaType="java.lang.String" 	mode="INOUT"/>
     <parameter property="intTamano" 						jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="intParaTipoAlineacionCod" 		jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="intParaTipoCompletaCod" 			jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="intParaEstadoCod" 				jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPk" class="java.util.Map">
   	 <parameter property="cursorLista" 					javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intPersEmpresaPk" 			javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemConfiguracion" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemConfiguracionDetalle" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parPorIntItemConfiguracion" class="java.util.Map">
   	 <parameter property="cursorLista" 					javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intItemConfiguracion" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <procedure id="grabar" parameterMap="parConfDetalle">
	{ call PKG_ARCHIVOCONFDETALLE.grabar(?,?,?,?,?,?,?,?,?,?,?,?)  }  
  </procedure>
  
  <procedure id="modificar" parameterMap="parConfDetalle" >
	{ call PKG_ARCHIVOCONFDETALLE.modificar(?,?,?,?,?,?,?,?,?,?,?,?)  }  
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPk" resultMap="resConfDetalle">
	{ call PKG_ARCHIVOCONFDETALLE.getListaPorPk(?,?,?,?) }  
  </procedure>
  
  <procedure id="getListaPorIntItemConfiguracion" parameterMap="parPorIntItemConfiguracion" resultMap="resConfDetalle">
	{ call PKG_ARCHIVOCONFDETALLE.getListaPorItem_archivoconf(?,?) }  
  </procedure>
    
</sqlMap>  