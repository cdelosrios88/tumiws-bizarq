<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="AfectoDaoIbatis">

	<typeAlias type="pe.com.tumi.credito.socio.captacion.domain.Afecto"
		alias="Afecto" />

	<resultMap id="resAfecto" class="Afecto">
		<result property="id.intPersEmpresaPk" column="pPERS_EMPRESA_N_PK" />
		<result property="id.intParaTipoCaptacionCod" column="pPARA_TIPOCAPTACION_N_COD" />
		<result property="id.intItem" column="pITEM_N" />
		<result property="id.intTipoCaptacionAfecto" column="pTIPOCAPTACIONAFECTA_N" />
		<result property="intIdValor" column="pVALOR_N" />
	</resultMap>

	<parameterMap id="parAfecto" class="Afecto">
		<parameter property="id.intPersEmpresaPk" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="INOUT" />
		<parameter property="id.intParaTipoCaptacionCod" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="INOUT" />
		<parameter property="id.intItem" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="INOUT" />
		<parameter property="id.intTipoCaptacionAfecto" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="INOUT" />
		<parameter property="intIdValor" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="INOUT" />
	</parameterMap>

	<parameterMap id="parPorPK" class="java.util.Map">
		<parameter property="cursorLista" javaType="java.sql.ResultSet"
			jdbcType="ORACLECURSOR" mode="OUT" />
		<parameter property="intPersEmpresaPk" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="IN" />
		<parameter property="intParaTipoCaptacionCod" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="IN" />
		<parameter property="intItem" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="IN" />
		<parameter property="intTipoCaptacionAfecto" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="IN" />
	</parameterMap>

	<parameterMap id="parPorPKCaptacion" class="java.util.Map">
		<parameter property="cursorLista" javaType="java.sql.ResultSet"
			jdbcType="ORACLECURSOR" mode="OUT" />
		<parameter property="intPersEmpresaPk" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="IN" />
		<parameter property="intParaTipoCaptacionCod" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="IN" />
		<parameter property="intItem" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="IN" />
	</parameterMap>

	<procedure id="grabar" parameterMap="parAfecto">
		{ call PKG_CONFCAPTAFECTO.grabar(?,?,?,?,?) }  
  </procedure>

	<procedure id="modificar" parameterMap="parAfecto">
		{ call PKG_CONFCAPTAFECTO.modificar(?,?,?,?,?) }  
  </procedure>

	<procedure id="getListaPorPk" parameterMap="parPorPK"
		resultMap="resAfecto">
		{ call PKG_CONFCAPTAFECTO.getListaPorPK(?,?,?,?,?) }  
  </procedure>

	<procedure id="getListaPorPkCaptacion" parameterMap="parPorPKCaptacion"
		resultMap="resAfecto">
		{ call PKG_CONFCAPTAFECTO.getListaPorPkCaptacion(?,?,?,?) }  
  </procedure>

	<procedure id="getListaAfectPorPkCaptacion" parameterMap="parPorPKCaptacion"
		resultMap="resAfecto">
		{ call PKG_CONFCAPTAFECTO.getListaAfectPorPkCaptacion(?,?,?,?) }
	</procedure>
</sqlMap>  