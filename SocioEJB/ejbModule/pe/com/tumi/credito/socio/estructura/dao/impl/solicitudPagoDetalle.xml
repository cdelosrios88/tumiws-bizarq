    <parameter property="intItemAdministraPadron"	javaType="java.lang.Integer"	jdbcType="NUMERIC"  	mode="IN"/>
  </parameterMap>
    
  <procedure id="grabar" parameterMap="parSolicitudPagoDetalle">
