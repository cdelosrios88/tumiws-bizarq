    <parameter property="intParaModalidadCod"		javaType="java.lang.Integer"	jdbcType="NUMERIC" 	mode="INOUT"/>
    <parameter property="intParaTipoSocioCod"		javaType="java.lang.Integer"	jdbcType="NUMERIC" 	mode="INOUT"/>
    <parameter property="intItemAdministraPadron" 	javaType="java.lang.Integer"	jdbcType="NUMERIC" 	mode="INOUT"/>
  </parameterMap>

  <parameterMap id="parSolicitudPagoDetalleLista" class="java.util.Map">
  	<parameter property="cursorLista" 				javaType="java.sql.ResultSet" 	jdbcType="ORACLECURSOR" mode="OUT"/>
  </parameterMap>
  
  <parameterMap id="parSolicitudPagoDetalleBusqueda" class="java.util.Map">
  	<parameter property="cursorLista" 				javaType="java.sql.ResultSet" 	jdbcType="ORACLECURSOR" mode="OUT"/>
  	<parameter property="intPeriodo"				javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="IN"/>
	<parameter property="intMes"					javaType="java.lang.Integer"	jdbcType="NUMERIC"  	mode="IN"/>    
