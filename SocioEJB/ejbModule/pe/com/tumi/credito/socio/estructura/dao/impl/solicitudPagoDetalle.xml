     <result property="intParaModalidadCod"			column="pPARA_MODALIDAD_N_COD" />
     <result property="intParaTipoSocioCod"			column="pPARA_TIPOSOCIO_N_COD" />
     <result property="intItemAdministraPadron"		column="pITEMADMINISTRAPADRON_N" />
