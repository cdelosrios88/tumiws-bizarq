  </procedure>
  
  <procedure id="getBusqueda" parameterMap="parSolicitudPagoDetalleBusqueda" resultMap="parSolicitudPagoDetalle">
	{ call PKG_SOLPAGODETALLE.getBusqueda(?,?,?,?,?,?,?,?,?)  }  
  </procedure>
</sqlMap>  
