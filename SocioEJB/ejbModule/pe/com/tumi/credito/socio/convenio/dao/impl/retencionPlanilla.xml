<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="RetencionPlanillaDaoIbatis">

  <typeAlias type="pe.com.tumi.credito.socio.convenio.domain.RetencionPlanilla" alias="RetencionPlanilla"/> 
	
  <resultMap id="resRetencionPlanilla" class="RetencionPlanilla" >
     <result property="id.intConvenio"  		column="pCONVENIO_N" />
     <result property="id.intItemConvenio"      column="pITEMCONVENIO_N" />
     <result property="id.intItemRetPlla"    	column="pITEMRETPLLA_N" />
     <result property="intParaEstPagoCod"     	column="pPARA_ESTADOPAGO_N_COD" />
     <result property="intNivel"   				column="pNIVEL_N" />
     <result property="intCodigo"   			column="pCODIGO_N" />
     <result property="intParaMesCod"   		column="pPARA_MES_N_COD" />
     <result property="intAnio"   				column="pANIO_N" />
     <result property="intParaTipoSocioCod"   	column="pPARA_TIPOSOCIO_N_COD" />
     <result property="intParaModalidadCod"   	column="pPARA_MODALIDAD_N_COD" />
  </resultMap>
  
  <parameterMap id="parRetencionPlla" class="RetencionPlanilla">
     <parameter property="id.intConvenio"		javaType="java.lang.Integer" 	jdbcType="NUMERIC"	mode="INOUT"/>
     <parameter property="id.intItemConvenio"	javaType="java.lang.Integer" 	jdbcType="NUMERIC"	mode="INOUT"/>
	 <parameter property="id.intItemRetPlla"	javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>    
     <parameter property="intParaEstPagoCod" 	javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>  
     <parameter property="intNivel" 			javaType="java.lang.Integer"	jdbcType="NUMERIC" 	mode="INOUT"/>
     <parameter property="intCodigo" 			javaType="java.lang.Integer"	jdbcType="NUMERIC" 	mode="INOUT"/>
     <parameter property="intParaMesCod" 		javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
     <parameter property="intAnio" 				javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
     <parameter property="intParaTipoSocioCod" 	javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
     <parameter property="intParaModalidadCod" 	javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPK" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intItemConvenio" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemItemConvenio" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemRetPlanilla" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parPorPKAdenda" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intIdConvenio" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intIdItemConvenio" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <procedure id="grabar" parameterMap="parRetencionPlla">
	{ call PKG_RETENCIONPLLA.grabar(?,?,?,?,?,?,?,?,?,?)  }
  </procedure>
  
  <procedure id="modificar" parameterMap="parRetencionPlla" >
	{ call PKG_RETENCIONPLLA.modificar(?,?,?,?,?,?,?,?,?,?)  }
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPK" resultMap="resRetencionPlanilla">
	{ call PKG_RETENCIONPLLA.getListaPorPk(?,?,?,?) }
  </procedure>
  
  <procedure id="getListaPorPkAdenda" parameterMap="parPorPKAdenda" resultMap="resRetencionPlanilla">
	{ call PKG_RETENCIONPLLA.getListaPorPkAdenda(?,?,?) }
  </procedure>
  
</sqlMap>  