<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="BancofondoDaoIbatis">

  <typeAlias type="pe.com.tumi.tesoreria.banco.domain.Bancofondo" alias="Bancofondo"/> 
	
  <resultMap id="resBancofondo" class="Bancofondo" >
	<result property="id.intEmpresaPk"   		column="pPERS_EMPRESA_N_PK" />
	<result property="id.intItembancofondo"   	column="pTESO_ITEMBANCOFONDO_N" />
    <result property="intBancoCod"      		column="pPARA_BANCO_N_COD" />
    <result property="intEmpresabancoPk"      	column="pPERS_EMPRESABANCO_N_PK" />
    <result property="intPersonabancoPk"      	column="pPERS_PERSONABANCO_N_PK" />
    <result property="intTipoFondoFijo"      	column="pPARA_TIPOFONDOFIJO_N_COD" />
    <result property="intMonedaCod"      		column="pPARA_MONEDA_N_COD" />
    <result property="strAbreviatura"      		column="pBAFO_ABREVIATURA_V" />
    <result property="strObservacion"      		column="pBAFO_OBSERVACION_V" />
    <result property="intEstadoCod"      		column="pPARA_ESTADO_N_COD" />
    <result property="tsFecharegistro"      	column="pBAFO_FECHAREGISTRO_D" />
  </resultMap>
  
  <parameterMap id="parBancofondo" class="Bancofondo">
	 <parameter property="id.intEmpresaPk"		javaType="java.lang.Integer"	jdbcType="NUMERIC"		mode="INOUT"/>
	 <parameter property="id.intItembancofondo"	javaType="java.lang.Integer"	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="intBancoCod"			javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="intEmpresabancoPk"	javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="intPersonabancoPk"	javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="intTipoFondoFijo"		javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="intMonedaCod"			javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="strAbreviatura"		javaType="java.lang.String" 	jdbcType="VARCHAR"		mode="INOUT"/>
     <parameter property="strObservacion"		javaType="java.lang.String" 	jdbcType="VARCHAR"		mode="INOUT"/>
     <parameter property="intEstadoCod"			javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="tsFecharegistro"		javaType="java.sql.Timestamp" 	jdbcType="TIMESTAMP"	mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPk" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" 	jdbcType="ORACLECURSOR" mode="OUT"/>
	 <parameter property="intEmpresaPk"			javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="IN"/>
	 <parameter property="intItembancofondo"	javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parPorBusqueda" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" 	jdbcType="ORACLECURSOR" mode="OUT"/>
	 <parameter property="intEstadoCod"			javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="IN"/>
     <parameter property="intMonedaCod"			javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parPorTFFYM" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" 	jdbcType="ORACLECURSOR" mode="OUT"/>
	 <parameter property="intEmpresaPk"			javaType="java.lang.Integer"	jdbcType="NUMERIC"		mode="IN"/>
	 <parameter property="intTipoFondoFijo"		javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="IN"/>
     <parameter property="intMonedaCod"			javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="IN"/>
  </parameterMap>
  
    <parameterMap id="parPorEmpyPers" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" 	jdbcType="ORACLECURSOR" mode="OUT"/>
	 <parameter property="intEmpresabancoPk"	javaType="java.lang.Integer"	jdbcType="NUMERIC"		mode="IN"/>
	 <parameter property="intPersonabancoPk"	javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="IN"/>
  </parameterMap>
  
  <procedure id="grabar" parameterMap="parBancofondo">
	{ call PKG_BANCOFONDO.grabar(?,?,?,?,?,?,?,?,?,?,?) }  
  </procedure>
  
  <procedure id="modificar" parameterMap="parBancofondo" >
	{ call PKG_BANCOFONDO.modificar(?,?,?,?,?,?,?,?,?,?,?) }  
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPk" resultMap="resBancofondo">
	{ call PKG_BANCOFONDO.getListaPorPk(?,?,?) }
  </procedure>
  
  <procedure id="getListaPorBusqueda" parameterMap="parPorBusqueda" resultMap="resBancofondo">
	{ call PKG_BANCOFONDO.getListaPorBusqueda(?,?,?) }
  </procedure>
  
  <procedure id="getListaPorTFFYM" parameterMap="parPorTFFYM" resultMap="resBancofondo">
	{ call PKG_BANCOFONDO.getListaPorTFFYM(?,?,?,?) }
  </procedure>

  <procedure id="getListaPorEmpyPers" parameterMap="parPorEmpyPers" resultMap="resBancofondo">
	{ call PKG_BANCOFONDO.getListaPorEmpYPers(?,?,?) }
  </procedure>    
</sqlMap>