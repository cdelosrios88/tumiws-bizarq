<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="ZonalDaoIbatis">

  <typeAlias type="pe.com.tumi.empresa.domain.Zonal" alias="Zonal"/> 
	
  <resultMap id="resZonal" class="Zonal" >
     <result property="id.intPersEmpresaPk"      	column="pPers_empresa_n_pk" />
     <result property="id.intIdzonal"  				column="pIdzonal_n" />
     <result property="intPersPersonaPk"      		column="pPers_persona_n_pk" />
     <result property="intIdTipoZonal"   			column="pIdtipozonal_n" />
     <result property="intPersPersonaResponsablePk" column="pPers_personaresponsable_n_pk" />
     <result property="sucursal.id.intIdSucursal"   column="pIdsucursal_n" />
     <result property="intIdEstado"    				column="pIdestado_n" />
  </resultMap>
  
  <parameterMap id="parZonal" class="Zonal">
     <parameter property="id.intPersEmpresaPk"			jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
	 <parameter property="id.intIdzonal"				jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>    
     <parameter property="intPersPersonaPk" 			jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="intIdTipoZonal"				jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
	 <parameter property="intPersPersonaResponsablePk"	jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>    
     <parameter property="sucursal.id.intIdSucursal" 	jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="intIdEstado" 					jdbcType="NUMERIC"  javaType="java.lang.Integer" 	mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPK" class="java.util.Map">
   	 <parameter property="cursorLista" 			jdbcType="ORACLECURSOR"  javaType="java.sql.ResultSet" 	mode="OUT"/>
   	 <parameter property="intPersEmpresaPk" 	jdbcType="NUMERIC"		 javaType="java.lang.Integer"  	mode="IN"/>
   	 <parameter property="intIdzonal" 			jdbcType="NUMERIC"		 javaType="java.lang.Integer"   mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parPorIdZonal" class="java.util.Map">
   	 <parameter property="cursorLista" 			jdbcType="ORACLECURSOR"  javaType="java.sql.ResultSet" 	mode="OUT"/>
   	 <parameter property="intIdzonal" 			jdbcType="NUMERIC"		 javaType="java.lang.Integer"   mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parPorIdPersona" class="java.util.Map">
   	 <parameter property="cursorLista" 			jdbcType="ORACLECURSOR"  javaType="java.sql.ResultSet" 	mode="OUT"/>
   	 <parameter property="intIdPersona" 		jdbcType="NUMERIC"		 javaType="java.lang.Integer"   mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parZonalBusqueda" class="java.util.Map">
  	 <parameter property="cursorLista" 					jdbcType="ORACLECURSOR" javaType="java.sql.ResultSet" 	mode="OUT"/>
     <parameter property="intPersEmpresaPk"				jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="IN"/>
	 <parameter property="intIdzonal"					jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="IN"/>    
     <parameter property="intPersPersonaPk" 			jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="IN"/>
     <parameter property="intIdTipoZonal"				jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="IN"/>
	 <parameter property="intPersPersonaResponsablePk"	jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="IN"/>    
     <parameter property="intIdSucursal" 				jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="IN"/>
     <parameter property="intIdEstado" 					jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="IN"/>
  </parameterMap>
   
  <procedure id="grabar" parameterMap="parZonal">
	{ call PKG_ZONAL.grabar(?,?,?,?,?,?,?) }
  </procedure>
  
  <procedure id="modificar" parameterMap="parZonal" >
	{ call PKG_ZONAL.modificar(?,?,?,?,?,?,?) }
  </procedure>
  
  <procedure id="getListaPorPK" parameterMap="parPorPK" resultMap="resZonal">
	{ call PKG_ZONAL.getListaPorPK(?,?,?) }
  </procedure>

  <procedure id="getListaPorIdZonal" parameterMap="parPorIdZonal" resultMap="resZonal">
	{ call PKG_ZONAL.getListaPorIdZonal(?,?) }
  </procedure>
  
  <procedure id="getListaPorIdPersona" parameterMap="parPorIdPersona" resultMap="resZonal">
	{ call PKG_ZONAL.getListaPorIdPersona(?,?) }
  </procedure>
  
  <procedure id="getListaDeBusqueda" parameterMap="parZonalBusqueda" resultMap="resZonal">
	{ call PKG_ZONAL.getListaDeBusqueda(?,?,?,?,?,?,?,?) }
  </procedure>
      
</sqlMap>  