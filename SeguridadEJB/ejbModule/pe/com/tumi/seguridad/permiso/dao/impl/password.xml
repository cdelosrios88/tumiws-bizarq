<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="PasswordDaoIbatis">

  <typeAlias type="pe.com.tumi.seguridad.permiso.domain.Password" alias="Password"/> 
	
  <resultMap id="resPassword" class="Password" >
     <result property="id.intEmpresaPk"      		column="pPers_empresa_n_pk" />
     <result property="id.intIdTransaccion"  		column="pIdtransaccion_n" />
     <result property="id.intIdPassword"  			column="pIdpassword_n" />
     <result property="strDetalle"      			column="pDetalle_v" />
     <result property="strContrasena"   			column="pContrasena_v" />
     <result property="tsFechaRegistro" 			column="pFecharegistro_d" />
     <result property="intParaEstadoCod"   			column="pEstado_n_cod" />
     <result property="tsFechaEliminacion"    		column="pFechaeliminacion_d" />
  </resultMap>
  
  <parameterMap id="parPorPK" class="java.util.Map">
   	 <parameter property="cursorLista" 			jdbcType="ORACLECURSOR"  javaType="java.sql.ResultSet" 	mode="OUT"/>
   	 <parameter property="intEmpresaPk" 		jdbcType="NUMERIC"		 javaType="java.lang.Integer"  	mode="IN"/>
   	 <parameter property="intIdTransaccion" 	jdbcType="NUMERIC"		 javaType="java.lang.Integer"  	mode="IN"/>
   	 <parameter property="intIdPassword" 		jdbcType="NUMERIC"		 javaType="java.lang.Integer"  	mode="IN"/>
  </parameterMap>

			
	<parameterMap id="parPorPKYPass" class="java.util.Map">
   	 <parameter property="cursorLista" 			jdbcType="ORACLECURSOR"  javaType="java.sql.ResultSet" 	mode="OUT"/>
   	 <parameter property="intEmpresaPk" 		jdbcType="NUMERIC"		 javaType="java.lang.Integer"  	mode="IN"/>
   	 <parameter property="intIdTransaccion" 	jdbcType="NUMERIC"		 javaType="java.lang.Integer"  	mode="IN"/>
   	 <parameter property="strContrasena" 		jdbcType="VARCHAR"		 javaType="java.lang.String"  	mode="IN"/>
  	</parameterMap>		
  
  <procedure id="getListaPorPk" parameterMap="parPorPK" resultMap="resPassword">
	{ call PKG_PASSWORD.getListaPorPK(?,?,?,?) }
  </procedure>
  
  <procedure id="getListaPorPkYPass" parameterMap="parPorPKYPass" resultMap="resPassword">
	{ call PKG_PASSWORD.getListaPorPkYPass(?,?,?,?)}
  </procedure>
  
  
</sqlMap>  