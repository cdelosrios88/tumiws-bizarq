<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="CobroPlanillasDaoIbatis">

  <typeAlias type="pe.com.tumi.cobranza.planilla.domain.CobroPlanillas" alias="CobroPlanillas"/> 
	
  <resultMap id="resCobroPlanillas" class="CobroPlanillas" >
	<result property="id.intEmpresa"   					column="pempresa" />
	<result property="id.intItemEfectuadoResumen"   	column="pitemefectuadoresumen" />
	<result property="id.intItemPagoPlanillas"      	column="pitempagoplanillas" />
	<result property="intPersEmpresaIngreso"      		column="pempresaingreso" />
	<result property="intItemIngresoGeneral"      		column="pitemingresogeneral" />
	<result property="bdMontoPago"      				column="pmontopago" />
	<result property="intParaTipoMoneda"      			column="ptipomoneda" />
	<result property="intParaEstado"      				column="pestado" />
  </resultMap>
  
  <parameterMap id="parCobroPlanillas" class="CobroPlanillas">
	 <parameter property="id.intEmpresa"				javaType="java.lang.Integer"	jdbcType="NUMERIC"		mode="INOUT"/>
	 <parameter property="id.intItemEfectuadoResumen"	javaType="java.lang.Integer"	jdbcType="NUMERIC"		mode="INOUT"/>
	 <parameter property="id.intItemPagoPlanillas"		javaType="java.lang.Integer"	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="intPersEmpresaIngreso"		javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="intItemIngresoGeneral"		javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="bdMontoPago"					javaType="java.math.BigDecimal" jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="intParaTipoMoneda"			javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="INOUT"/>
     <parameter property="intParaEstado"				javaType="java.lang.Integer" 	jdbcType="NUMERIC"		mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPk" class="java.util.Map">
   	 <parameter property="cursorLista" 					javaType="java.sql.ResultSet" 	jdbcType="ORACLECURSOR" mode="OUT"/>
	 <parameter property="intEmpresa"					javaType="java.lang.Integer"  	jdbcType="NUMERIC"	   	mode="IN"/>
	 <parameter property="intItemEfectuadoResumen"		javaType="java.lang.Integer"  	jdbcType="NUMERIC"	   	mode="IN"/>
	 <parameter property="intItemPagoPlanillas"			javaType="java.lang.Integer"  	jdbcType="NUMERIC"	   	mode="IN"/>
  </parameterMap>

  <parameterMap id="parPorEfectuadoResumen" class="java.util.Map">
   	 <parameter property="cursorLista" 					javaType="java.sql.ResultSet" 	jdbcType="ORACLECURSOR" mode="OUT"/>
	 <parameter property="intEmpresa"					javaType="java.lang.Integer"  	jdbcType="NUMERIC"	   	mode="IN"/>
	 <parameter property="intItemEfectuadoResumen"		javaType="java.lang.Integer"  	jdbcType="NUMERIC"	   	mode="IN"/>
  </parameterMap>
  
  
  <procedure id="grabar" parameterMap="parCobroPlanillas">
	{ call PKG_COBROPLANILLAS.grabar(?,?,?,?,?,?,?,?) }  
  </procedure>
  
  <procedure id="modificar" parameterMap="parCobroPlanillas" >
	{ call PKG_COBROPLANILLAS.modificar(?,?,?,?,?,?,?,?) }  
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPk" resultMap="resCobroPlanillas">
	{ call PKG_COBROPLANILLAS.getListaPorPk(?,?,?) }
  </procedure>

  <procedure id="getListaPorEfectuadoResumen" parameterMap="parPorEfectuadoResumen" resultMap="resCobroPlanillas">
	{ call PKG_COBROPLANILLAS.getListaPorEfectuadoResumen(?,?,?) }
  </procedure>
</sqlMap>