   	 <parameter property="intItemSolicitud" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <procedure id="grabar" parameterMap="parConfServUsuario">
	{ call PKG_CONFUSUARIO.grabar(?,?,?,?,?,?,?,?,?)  }  
  </procedure>
  
  <procedure id="modificar" parameterMap="parConfServUsuario" >
	{ call PKG_CONFUSUARIO.modificar(?,?,?,?,?,?,?,?,?)  }  
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPk" resultMap="resConfServUsuario">
	{ call PKG_CONFUSUARIO.getListaPorPk(?,?,?,?) }  
  </procedure>
  
  <procedure id="getListaPorCabecera" parameterMap="parPorCabecera" resultMap="resConfServUsuario">
	{ call PKG_CONFUSUARIO.getListaPorCabecera(?,?,?) }  
  </procedure>
</sqlMap>  