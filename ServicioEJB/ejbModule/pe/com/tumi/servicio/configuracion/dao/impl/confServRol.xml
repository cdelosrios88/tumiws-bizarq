   	 <parameter property="intItemSolicitud" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <procedure id="grabar" parameterMap="parConfServRol">
	{ call PKG_CONFROL.grabar(?,?,?,?)  }  
  </procedure>
  
  <procedure id="modificar" parameterMap="parConfServRol" >
	{ call PKG_CONFROL.modificar(?,?,?,?)  }  
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPk" resultMap="resConfServRol">
	{ call PKG_CONFROL.getListaPorPk(?,?,?,?) }  
  </procedure>    
  
  <procedure id="getListaPorCabecera" parameterMap="parPorCabecera" resultMap="resConfServRol">
	{ call PKG_CONFROL.getListaPorCabecera(?,?,?) }  
  </procedure>
  
