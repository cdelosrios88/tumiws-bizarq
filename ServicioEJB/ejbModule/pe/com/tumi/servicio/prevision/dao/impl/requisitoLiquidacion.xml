   	 <parameter property="intPersonaBeneficiario" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>  
    	
  <procedure id="grabar" parameterMap="parRequisitoLiquidacion">
	{ call PKG_REQUISITOLIQUIDACION.grabar(?,?,?,?,?,?,?,?,?,?,?,?) }
  </procedure>
  
  <procedure id="modificar" parameterMap="parRequisitoLiquidacion" >
	{ call PKG_REQUISITOLIQUIDACION.modificar(?,?,?,?,?,?,?,?,?,?,?,?) }
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPk" resultMap="resRequisitoLiquidacion">
	{ call PKG_REQUISITOLIQUIDACION.getListaPorPk(?,?,?,?) }
  </procedure>

  <procedure id="getListaPorExpediente" parameterMap="parPorExpediente" resultMap="resRequisitoLiquidacion">
	{ call PKG_REQUISITOLIQUIDACION.getListaPorExpediente(?,?,?) }
  </procedure>
  
  <procedure id="getListaPorPkExpedienteLiquidacionYRequisitoDetalle" parameterMap="parPorPkExpedienteLiquidacionYRequisitoDetalle" resultMap="resRequisitoLiquidacion">
	{ call PKG_REQUISITOLIQUIDACION.getPorExpLiqYReqAutDetalle(?,?,?,?,?,?,?) }
  </procedure>      
</sqlMap>