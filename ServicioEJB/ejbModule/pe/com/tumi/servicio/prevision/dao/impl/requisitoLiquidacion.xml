     <result property="intParaItemHistorico"  			column="pParaItemHistorico"/>
     <result property="intParaEstado"     				column="pParaEstado" />
     <result property="tsFechaRequisito"   				column="pPFechaRequisito" />
     <result property="intPersonaBeneficiarioId"   		column="pPersonaBeneficiarioId" />
  </resultMap>
  
  <parameterMap id="parRequisitoLiquidacion" 				class="RequisitoLiquidacion">
     <parameter property="id.intPersEmpresaLiquidacion"	jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="id.intItemExpediente"			jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="id.intItemRequisito"			jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="intPersEmpresaPk"				jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
