<sqlMap namespace="CancelacionCreditoDaoIbatis">

  <typeAlias type="pe.com.tumi.servicio.solicitudPrestamo.domain.CancelacionCredito" alias="CancelacionCredito"/> 
	
  <resultMap id="resCancelacionCredito" class="CancelacionCredito" >
  	 <result property="id.intPersEmpresaPk"  		column="pPers_empresa"/>
  	 <result property="id.intCuentaPk"  			column="pCsoc_cuenta"/>
  	 <result property="id.intItemExpediente"  		column="pCser_itemexpediente"/>
  	 <result property="id.intItemDetExpediente"  	column="pCser_itemderexpediente"/>
     <result property="id.intItemCancelacion"  		column="pCser_itemcancelacion"/>
     <result property="intItemExpediente"  			column="pCacr_itemexpediente"/>
     <result property="bdMontoCancelado"     		column="pCacr_montocancelado"/>
     <result property="intItemDetExpediente"     		column="pCacr_itemdetexpediente"/>
  </resultMap>
  
  <parameterMap id="parCancelacionCredito" class="CancelacionCredito">
     <parameter property="id.intPersEmpresaPk"			jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="id.intCuentaPk"				jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="id.intItemExpediente"			jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="id.intItemDetExpediente"		jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
	 <parameter property="id.intItemCancelacion"		jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>    
     <parameter property="intItemExpediente" 			jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>  
     <parameter property="bdMontoCancelado" 			jdbcType="NUMERIC"  	javaType="java.math.BigDecimal" mode="INOUT"/>
     <parameter property="intItemDetExpediente" 		jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPk" class="java.util.Map">
   	 <parameter property="cursorLista" 				javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intPersEmpresaPk" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intCuenta" 				javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemExpediente" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemDetExpediente" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="id.intItemCancelacion" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parPorExpedienteCredito" class="java.util.Map">
   	 <parameter property="cursorLista" 				javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intPersEmpresaPk" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intCuenta" 				javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemExpediente" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemDetExpediente" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>

   
  <procedure id="getListaPorExpedienteCredito" parameterMap="parPorExpedienteCredito" resultMap="resCancelacionCredito">
	{ call PKG_CANCELACIONCREDITO.getListaPorExpedienteCredito(?,?,?,?,?) }
  </procedure>
  
  <procedure id="grabar" parameterMap="parCancelacionCredito" resultMap="resCancelacionCredito">
	{ call PKG_CANCELACIONCREDITO.grabar(?,?,?,?,?,?,?,?) }
  </procedure>
  
  <procedure id="modificar" parameterMap="parCancelacionCredito" resultMap="resCancelacionCredito">
	{ call PKG_CANCELACIONCREDITO.modificar(?,?,?,?,?,?,?,?) }
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPk" resultMap="resCancelacionCredito">
	{ call PKG_CANCELACIONCREDITO.getListaPorPk(?,?,?,?,?,?) }
  </procedure>
  
</sqlMap>