<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="TipoComprobanteDaoIbatis">

  <typeAlias type="pe.com.tumi.persona.empresa.domain.TipoComprobante" alias="TipoComprobante"/> 
	
  <resultMap id="resTipoComprobante" class="TipoComprobante" >
     <result property="id.intIdPersona"      		column="pPERSONA_N" />
     <result property="id.intItemTipoComprobante"   column="pITEMCOMPROBANTE_N_COD" />
     <result property="intTipoComprobanteCod"    	column="pTIPOCOMPROBANTE_N_COD" />
     <result property="intEstadoCod"   				column="pESTADO_N_COD" />
     <result property="tsFechaEliminacion" 			column="pFECHAELIMINACION_D" />
  </resultMap>
  
  <parameterMap id="parTipoComprobante" class="TipoComprobante">
     <parameter property="id.intIdPersona"				jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
	 <parameter property="id.intItemTipoComprobante"	jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>  
     <parameter property="intTipoComprobanteCod" 		jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="intEstadoCod" 				jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="tsFechaEliminacion" 			jdbcType="TIMESTAMP"  	javaType="java.sql.Timestamp" 	mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPK" class="java.util.Map">
   	 <parameter property="cursorLista" 					javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intIdPersona" 				javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemComprobante" 			javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parPorIdPersona" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intIdPersona" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
   
  <procedure id="grabar" parameterMap="parTipoComprobante">
	{ call PKG_COMPROBANTE.grabar(?,?,?,?,?)  }  
  </procedure>
  
  <procedure id="modificar" parameterMap="parTipoComprobante" >
	{ call PKG_COMPROBANTE.modificar(?,?,?,?,?)  }  
  </procedure>
  
  <procedure id="getListaPorPK" parameterMap="parPorPK" resultMap="resTipoComprobante">
	{ call PKG_COMPROBANTE.getListaPorPK(?,?,?) }  
  </procedure>
  
  <procedure id="getListaPorIdPersona" parameterMap="parPorIdPersona" resultMap="resTipoComprobante">
	{ call PKG_COMPROBANTE.getListaPorIdPersona(?,?) }  
  </procedure>
    
</sqlMap>  