	{ call PKG_CUADROCOMP.grabar(?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?) }  
  </procedure>
  
  <procedure id="modificar" parameterMap="parCuadroComparativo" >
	{ call PKG_CUADROCOMP.modificar(?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?) }  
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPk" resultMap="resCuadroComparativo">
