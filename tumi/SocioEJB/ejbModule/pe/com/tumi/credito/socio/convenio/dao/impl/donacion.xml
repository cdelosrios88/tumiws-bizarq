<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="DonacionDaoIbatis">

  <typeAlias type="pe.com.tumi.credito.socio.convenio.domain.DonacionRegalia" alias="DonacionRegalia"/> 
	
  <resultMap id="resDonacion" class="DonacionRegalia" >
     <result property="id.intConvenio"  		column="pCONVENIO_N" />
     <result property="id.intItemConvenio"      column="pITEMCONVENIO_N" />
     <result property="id.intItemDonacion"    	column="pITEMDONACION_N" />
     <result property="intParaTipoDonacionCod"  column="pPARA_TIPODONACION_N_COD" />
     <result property="strDescripcion"   		column="pDESCRIPCION_V" />
     <result property="intParaTipoMonedaCod"   	column="pPARA_MONEDA_N_COD" />
     <result property="bdMonto"   				column="pMONTO_N" />
     <result property="intDctoCompra"   		column="pDSCTOCOMPRA_N" />
     <result property="intParaEstadoPagoCod"   	column="pPARA_ESTADOPAGO_N_COD" />
     <result property="intParaEstadoCod"   		column="pPARA_ESTADO_N_COD" />
     <result property="intPersonaRegistraPk"   	column="pPERS_PERSONAREGISTRA_N" />
     <result property="tsFechaRegistra"   		column="pFECHAREGISTRA_D" />
     <result property="intPersonaEliminaPk"   	column="pPERS_PERSONAELIMINA_N" />
     <result property="tsFechaEliminacion"   	column="pFECHAELIMINACION_D" />
  </resultMap>
  
  <parameterMap id="parDonacion" class="DonacionRegalia">
     <parameter property="id.intConvenio"			javaType="java.lang.Integer" 	jdbcType="NUMERIC"	mode="INOUT"/>
     <parameter property="id.intItemConvenio"		javaType="java.lang.Integer" 	jdbcType="NUMERIC"	mode="INOUT"/>
	 <parameter property="id.intItemDonacion"		javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>    
     <parameter property="intParaTipoDonacionCod" 	javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>  
     <parameter property="strDescripcion" 			javaType="java.lang.String"		jdbcType="VARCHAR" 	mode="INOUT"/>
     <parameter property="intParaTipoMonedaCod" 	javaType="java.lang.Integer"	jdbcType="NUMERIC" 	mode="INOUT"/>
     <parameter property="bdMonto" 					javaType="java.math.BigDecimal"	jdbcType="NUMERIC"  mode="INOUT"/>
     <parameter property="intDctoCompra" 			javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
     <parameter property="intParaEstadoPagoCod" 	javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
     <parameter property="intParaEstadoCod" 		javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
     <parameter property="intPersonaRegistraPk" 	javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
     <parameter property="tsFechaRegistra" 			javaType="java.sql.Timestamp"	jdbcType="TIMESTAMP" mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPK" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intItemConvenio" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemItemConvenio" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemDonacion" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parPorPKAdenda" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intIdConvenio" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intIdItemConvenio" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <procedure id="grabar" parameterMap="parDonacion">
	{ call PKG_DONACION.grabar(?,?,?,?,?,?,?,?,?,?,?,?)  }
  </procedure>
  
  <procedure id="modificar" parameterMap="parDonacion" >
	{ call PKG_DONACION.modificar(?,?,?,?,?,?,?,?,?,?,?,?)  }
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPK" resultMap="resDonacion">
	{ call PKG_DONACION.getListaPorPk(?,?,?,?) }
  </procedure>
  
  <procedure id="getListaPorPkAdenda" parameterMap="parPorPKAdenda" resultMap="resDonacion">
	{ call PKG_DONACION.getListaPorPkAdenda(?,?,?) }
  </procedure>
  
</sqlMap>  