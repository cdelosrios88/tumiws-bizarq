<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="PerfilDetalleDaoIbatis">

  <typeAlias type="pe.com.tumi.credito.socio.convenio.domain.PerfilDetalle" alias="PerfilDetalle"/> 
	
  <resultMap id="resPerfilDetalle" class="PerfilDetalle" >
     <result property="id.intItemAdendaPerfil"  column="pITEMADENDAPERFIL_N" />
     <result property="id.intPersEmpresaPk"     column="pPERS_EMPRESA_N_PK" />
     <result property="id.intSeguPerfilPk"   	column="pSEGU_PERFIL_N_PK" />
     <result property="id.intParaValidacionCod" column="pPARA_VALIDACION_N_COD" />
     <result property="intValor"   				column="pVALOR_N" />
  </resultMap>
  
  <parameterMap id="parPerfilDetalle" class="PerfilDetalle">
     <parameter property="id.intItemAdendaPerfil" 	javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>  
     <parameter property="id.intPersEmpresaPk" 		javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
     <parameter property="id.intSeguPerfilPk" 		javaType="java.lang.Integer"	jdbcType="NUMERIC" 	mode="INOUT"/>
     <parameter property="id.intParaValidacionCod" 	javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
     <parameter property="intValor" 				javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPK" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intItemAdendaPerfil" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intPersEmpresaPk" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intSeguPerfilPk" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intParaValidacionCod" javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
   
  <parameterMap id="parPorPKPerfil" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intItemAdendaPerfil" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <procedure id="grabar" parameterMap="parPerfilDetalle">
	{ call PKG_ADENDAPERFILDETALLE.grabar(?,?,?,?,?)  }  
  </procedure>
  
  <procedure id="modificar" parameterMap="parPerfilDetalle" >
	{ call PKG_ADENDAPERFILDETALLE.modificar(?,?,?,?,?)  }  
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPK" resultMap="resPerfilDetalle">
	{ call PKG_ADENDAPERFILDETALLE.getListaPorPk(?,?,?,?,?) }  
  </procedure>
  
  <procedure id="getListaPorPkPerfil" parameterMap="parPorPKPerfil" resultMap="resPerfilDetalle">
	{ call PKG_ADENDAPERFILDETALLE.getListaPorPkPerfil(?,?) }  
  </procedure>
    
</sqlMap>  
