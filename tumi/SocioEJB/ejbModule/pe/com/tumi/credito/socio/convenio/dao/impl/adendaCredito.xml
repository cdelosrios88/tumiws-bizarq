<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="AdendaCreditoDaoIbatis">

  <typeAlias type="pe.com.tumi.credito.socio.convenio.domain.AdendaCredito" alias="AdendaCredito"/> 
	
  <resultMap id="resAdendaCredito" class="AdendaCredito">
     <result property="id.intConvenio"  			column="pCONVENIO_N" />
     <result property="id.intItemConvenio"      	column="pITEMCONVENIO_N" />
     <result property="id.intPersEmpresaPk"    		column="pPERS_EMPRESA_N" />
     <result property="id.intParaTipoCreditoCod"  	column="pPARA_TIPOCREDITO_N" />
     <result property="id.intItemCredito"			column="pITEMCREDITO_N" />
     <result property="intParaEstadoCod"   			column="pPARA_ESTADO_N" />
     <result property="intPersPersonaRegistraPk"	column="pPERSONAREGISTRA_N" />
     <result property="tsFechaRegistra"   			column="pFECHAREGISTRA_D" />
     <result property="intPersPersonaEliminaPk"   	column="pPERSONAELIMINA_N" />
     <result property="tsFechaEliminacion"   		column="pFECHAELIMINACION_D" />
  </resultMap>
  
  <parameterMap id="parAdendaCredito" class="AdendaCredito">
     <parameter property="id.intConvenio"				javaType="java.lang.Integer" 	jdbcType="NUMERIC"	mode="INOUT"/>
     <parameter property="id.intItemConvenio"			javaType="java.lang.Integer" 	jdbcType="NUMERIC"	mode="INOUT"/>
	 <parameter property="id.intPersEmpresaPk"			javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>  
	 <parameter property="id.intParaTipoCreditoCod" 	javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>  
     <parameter property="id.intItemCredito" 			javaType="java.lang.Integer"	jdbcType="NUMERIC"  mode="INOUT"/>  
     <parameter property="intParaEstadoCod" 			javaType="java.lang.Integer"	jdbcType="NUMERIC" 	mode="INOUT"/>
     <parameter property="intPersPersonaRegistraPk" 	javaType="java.lang.Integer"	jdbcType="NUMERIC" 	mode="INOUT"/>
     <parameter property="tsFechaRegistra" 				javaType="java.sql.Timestamp"	jdbcType="TIMESTAMP" mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPK" class="java.util.Map">
   	 <parameter property="cursorLista" 				javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intItemConvenio" 			javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemItemConvenio" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intPersEmpresaPk" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intParaTipoCreditoCod" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemCredito" 			javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <parameterMap id="parPorPKAdenda" class="java.util.Map">
   	 <parameter property="cursorLista" 			javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intIdConvenio" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intIdItemConvenio" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <procedure id="grabar" parameterMap="parAdendaCredito">
	{ call PKG_ADENDACREDITOS.grabar(?,?,?,?,?,?,?,?)  }
  </procedure>
  
  <procedure id="modificar" parameterMap="parAdendaCredito" >
	{ call PKG_ADENDACREDITOS.modificar(?,?,?,?,?,?,?,?)  }
  </procedure>
  
  <procedure id="getListaPorPk" parameterMap="parPorPK" resultMap="resAdendaCredito">
	{ call PKG_ADENDACREDITOS.getListaPorPk(?,?,?,?,?,?) }
  </procedure>
  
  <procedure id="getListaPorPkAdenda" parameterMap="parPorPKAdenda" resultMap="resAdendaCredito">
	{ call PKG_ADENDACREDITOS.getListaPorPkAdenda(?,?,?) }
  </procedure>
  
</sqlMap>  