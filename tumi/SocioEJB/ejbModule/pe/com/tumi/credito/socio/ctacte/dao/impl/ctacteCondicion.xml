		<result property="id.intParaTipoCtacteCod" column="pPARA_TIPOCTACTE_N_COD" />
		<result property="id.intItemCtacte" column="pITEMCTACTE_N" />
		<result property="id.intParaCondicionSocioCod" column="pPARA_CONDICIONSOCIO_N_COD" />
		<result property="intValor" column="pVALOR_N" />
	</resultMap>

	<parameterMap id="parCtaCteCondicion" class="CtaCteCondicion">
		<parameter property="id.intPersEmpresaPk" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="INOUT" />
		<parameter property="id.intParaTipoCtacteCod" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="INOUT" />
		<parameter property="id.intItemCtacte" javaType="java.lang.Integer"
