		<parameter property="intParaCondicionSocioCod" javaType="java.lang.Integer"
			jdbcType="NUMERIC" mode="IN" />
	</parameterMap>

	<procedure id="grabar" parameterMap="parCtaCteCondicion">
		{ call PKG_CTACTECONDICION.grabar(?,?,?,?,?) }  
  </procedure>

	<procedure id="modificar" parameterMap="parCtaCteCondicion">
		{ call PKG_CTACTECONDICION.modificar(?,?,?,?,?) }  
  </procedure>

	<procedure id="getListaPorPk" parameterMap="parPorPK"
