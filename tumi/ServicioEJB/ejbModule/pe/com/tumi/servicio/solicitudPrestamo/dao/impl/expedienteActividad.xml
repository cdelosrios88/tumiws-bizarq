<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="ExpedienteActividadDaoIbatis">

  <typeAlias type="pe.com.tumi.servicio.solicitudPrestamo.domain.ExpedienteActividad" alias="ExpedienteActividad"/> 

  <resultMap id="resExpedienteActividad" class="ExpedienteActividad" >
  	 <result property="id.intPersEmpresaPk"  		column="pPers_Empresa_n_Pk" />
  	 <result property="id.intCuentaPk"  			column="pCsoc_Cuenta_n_Pk" />
  	 <result property="id.intItemExpediente"  		column="pCser_Itemexpediente_n" />
  	 <result property="id.intItemDetExpediente"  	column="pCser_Itemdetexpediente_n" />
     <result property="intParaTipoSolicitudCod"  	column="pPara_Tiposolicitudact_n_Cod" />
     <result property="bdMontoCancelado"  			column="pExac_Montocancelado_n" />
  </resultMap>
  
  <parameterMap id="parExpedienteActividad" class="ExpedienteActividad">
     <parameter property="id.intPersEmpresaPk"			jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="id.intCuentaPk"				jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="id.intItemExpediente"			jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
     <parameter property="id.intItemDetExpediente"		jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>
	 <parameter property="intParaTipoSolicitudCod"		jdbcType="NUMERIC"  	javaType="java.lang.Integer" 	mode="INOUT"/>    
     <parameter property="bdMontoCancelado" 			jdbcType="NUMERIC"  	javaType="java.math.BigDecimal" 	mode="INOUT"/>
  </parameterMap>
  
  <parameterMap id="parPorPkExpedienteCredito" class="java.util.Map">
   	 <parameter property="cursorLista" 				javaType="java.sql.ResultSet" jdbcType="ORACLECURSOR"  mode="OUT"/>
   	 <parameter property="intPersEmpresaPk" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intCuentaPk" 				javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemExpediente" 		javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
   	 <parameter property="intItemDetExpediente" 	javaType="java.lang.Integer"  jdbcType="NUMERIC" 	   mode="IN"/>
  </parameterMap>
  
  <procedure id="grabar" parameterMap="parExpedienteActividad"> 
	{ call PKG_CSE_EXPEDIENTEACTIVIDAD.grabar(?,?,?,?,?,?) }
  </procedure>
  
  <procedure id="modificar" parameterMap="parExpedienteActividad" >
	{ call PKG_CSE_EXPEDIENTEACTIVIDAD.modificar(?,?,?,?,?,?) }
  </procedure>

  <procedure id="getListaPorExpedienteCredito" parameterMap="parPorPkExpedienteCredito" resultMap="resExpedienteActividad">
	{ call PKG_CSE_EXPEDIENTEACTIVIDAD.getListaPorExpedienteCredito(?,?,?,?,?)}
  </procedure>
</sqlMap>
